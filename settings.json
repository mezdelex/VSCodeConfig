{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.associations": {
        "**/*.html": "html",
        "**/templates/*/*.html": "django-html",
        "**/templates/*": "django-html",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements"
    },
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "workbench.colorTheme": "Gruvbox Material Dark",
    "editor.fontLigatures": true,
    "editor.fontFamily": "'CaskaydiaCove NF'",
    "editor.suggestSelection": "first",
    "editor.inlineSuggest.enabled": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "intelephense.format.braces": "k&r",
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "none",
    "beautify.language": {
        "html": ["htm", "html", "django-html"]
    },
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vetur.format.defaultFormatter.ts": "prettier-tslint",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "html.format.wrapAttributes": "force-aligned",
    "vetur.format.defaultFormatter.html": "js-beautify-html",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "C_Cpp.clang_format_fallbackStyle": "WebKit",
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.autofetch": true,
    "tabnine.experimentalAutoImports": true,
    "workbench.startupEditor": "newUntitledFile",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"c:\\Users\\Mezdelex\\.vscode\\extensions\\gabrielbb.vscode-lombok-1.0.1\\server\\lombok.jar\"",
    "java.project.importOnFirstTimeStartup": "automatic",
    "redhat.telemetry.enabled": true,
    "terminal.external.windowsExec": "wt",
    "terminal.explorerKind": "external",
    "powershell.integratedConsole.showOnStartup": false,
    "security.workspace.trust.untrustedFiles": "open",
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true
    }
}
